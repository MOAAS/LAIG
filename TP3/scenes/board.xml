    <animations>
        <animation id="jumping" maxLoops="3">
            <keyframe instant="0.25">
                <translate x="0" y="0.75" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="0.5">
                <translate x="0" y="0.0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
        </animation>

        <animation id="invalidmove" maxLoops="1">
            <keyframe instant="0.2">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="0.8" y="0.5" z="0.8" />  
            </keyframe>
            <keyframe instant="1">
                <translate x="0" y="0.0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
        </animation>

        <animation id="shrinking" maxLoops="1">
            <keyframe instant="0.9">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="0.1" y="0.1" z="0.1" />  
            </keyframe>
            <keyframe instant="1">
                <translate x="0" y="0.0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="0.01" y="0.01" z="0.01" />  
            </keyframe>
        </animation>

        <animation id="buttonpress" maxLoops="1">
            <keyframe instant="0.2">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="0.2" z="1" />  
            </keyframe>
            <keyframe instant="1">
                <translate x="0" y="0.0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
        </animation>

        <animation id="buttonToggleDown" maxLoops="1">
            <keyframe instant="0.0">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="0.2">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="0.2" z="1" />  
            </keyframe>
        </animation>

        <animation id="buttonToggleUp" maxLoops="1">
            <keyframe instant="0.0">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="0.2" z="1" />  
            </keyframe>
            <keyframe instant="0.2">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1.0" z="1" />  
            </keyframe>
        </animation>
        
    </animations>

